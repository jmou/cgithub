<% layout("./layout.eta") %>

<%~ include("./_nav.eta", it) %>

<div>
  <% if (it.language) { %>
    <div>language: <%= it.language %></div>
  <% } %>
  <% if (it.size !== undefined) { %>
    <div>size: <%= it.size %> bytes</div>
  <% } %>
  <div>
    <a href="/<%= it.repo.owner %>/<%= it.repo.name %>/raw/<%= it.branch %>/<%= it.path %>">Raw</a>
  </div>
</div>

<pre class="box"><code><%= it.rawContent %></code></pre>
